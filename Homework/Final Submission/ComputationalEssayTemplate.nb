   3.77071884575416*^9, {3.7707192101730976`*^9, 3.7707192111888857`*^9}, {
   3.7707192927440643`*^9, 3.7707193645529537`*^9}, {3.7707194359205666`*^9, 
   3.7707194860255003`*^9}},ExpressionUUID->"7ed8732d-7621-462e-a0c8-\
